      display: block;
    }

    /* PACKS GRID */
    .packs-grid {
      display: grid;
      grid-template-columns: 1fr 1fr;
      gap: 40px;
      align-items: center;
    }

    /* IMAGE CONTROL */
    .packs-image {
      max-height: 320px;
      overflow: hidden;
      border-radius: 12px;
    }

    .packs-image img {
      width: 100%;
      height: 100%;
      object-fit: contain;
      display: block;
    }

    /* PACK LIST */
    .packs-content ul {
      list-style: none;
      padding: 0;
      margin: 20px 0 0;
    }

    .packs-content li {
      margin-bottom: 16px;
      line-height: 1.4;
    }

    .footer-actions {
      display: flex;
      justify-content: center;
      align-items: center;
      gap: 20px;
